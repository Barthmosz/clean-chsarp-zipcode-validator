<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Title>Clean Zip Code Validator</Title>
    <Version>1.0.1</Version>
    <Authors>Barthmosz</Authors>
    <Description>A simple and clean zip code validator.</Description>
    <PackageProjectUrl>https://github.com/Barthmosz/clean-chsarp-zipcode-validator</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Barthmosz/clean-chsarp-zipcode-validator</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Zip;Code;Zip-Code;Validator;Validation</PackageTags>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
